pipeline {
    agent any

    // blocos de execucao
    stages {
        stage ('Run Test'){
            steps{
                robot 'Tests/Quote/Quote.robot'
            }
        }
        stage('Publish Results') {
            steps {
                script {
                    // Publicar relat√≥rios do Robot Framework no Jenkins
                    robot publish --output results/output.xml --reportdir results --logtitle 'Test Execution Report'
                }   
                   
            }
        }
    
    }
}