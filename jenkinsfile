pipeline {
    agent any

    // blocos de execucao
    stages {
        stage ('Checkout') {
            steps {
                git 'https://github.com/orafaelkin/BDD-FORM.git'
            }
        }
      
        stage ('Run Test'){
            steps{
                sh 'robot  Tests/Quote/Quote.robot'
            }
        }
    }
    
}